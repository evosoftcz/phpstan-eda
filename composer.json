{
	"name": "evosoftcz/phpstan-eda",
	"type": "phpstan-extension",
	"description": "Eda class reflection extension for PHPStan",
	"license": [
		"MIT"
	],
	"require": {
		"php": "^8.1",
		"phpstan/phpstan": "^2.0"
	},
	"require-dev": {
		"php-parallel-lint/php-parallel-lint": "^1.2",
		"phpstan/phpstan-phpunit": "^2.0",
		"phpstan/phpstan-strict-rules": "^2.0",
		"phpunit/phpunit": "^9.6"
	},
	"config": {
		"sort-packages": true
	},
	"extra": {
		"phpstan": {
			"includes": [
				"extension.neon"
			]
		}
	},
	"autoload": {
		"psr-4": {
			"PHPStan\\": "src/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
